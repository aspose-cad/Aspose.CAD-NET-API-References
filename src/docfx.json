{
  "metadata": [
    {
      "dest": "api",
      "src": [
        {
		  //"src": "./",
          "files": [
            //"bootstrap/Docfx.Boostrap/bin/Debug/net8.0/Aspose.CAD.dll"
			"bootstrap/Docfx.Boostrap/bin/Debug/net8.0/System.Composition.Convention.dll"
          ]
        }
      ],	  
      "noRestore": false,
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "namespaceLayout": "nested",
	  "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
		"src": ".",  
        "files": [
		  "*.{md,yml}",
          "content/**/*.{md,yml}",
		  "api/**/*.{md,yml}"
        ],
        "exclude": [
          //"../_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
	  {
        "files": [
          "images/favicon.ico"
        ],
        "src": ".",
        "dest": "."
      }
    ],
    "output": "../_site",
    "template": [
      "default",
	  "templates/aspose-modern"
    ],
	//"customLinkResolver": "AsposeHrefGenerator",
    "postProcessors": ["UrlCustomizationProcessor"],
    "globalMetadata": {
      "_appName": "Aspose.CAD .NET SDK",
      "_appTitle": "Aspose.CAD for .NET API Reference",
      "_enableSearch": true,
	  "_enableNewTab": true,
      "pdf": true,
	  
	  "_trailingSlash": true,
	  "_lowerCaseFiles": true,
	  "_ctorToClassName": true,
	  "_symbolsSeparator": "/",
	  "_virtualPath": "/cad/net/",
	  "_suppressPrefixes": [ "/api/" ],
	  "_suppressExtensions": [ ".html" ]
    },
	"sitemap": {
      "baseUrl": "https://reference.aspose.com/cad/net/",
      "priority": 0.1,
      "changefreq": "monthly"
    }
  }
}