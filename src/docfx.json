{
  "metadata": [
    {
      "dest": "api",
      "src": [
        {
          "files": [
            "bootstrap/bin/Release/net8.0/Aspose.CAD.dll"
          ]
        }
      ],	  
      "noRestore": false,
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
	  "namespaceLayout": "nested",
	  "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
		  "*.{md,yml}",
          "content/**/*.{md,yml}",
		  "api/**/*.{md,yml}"
        ],
        "exclude": [
          "../_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
	  {
        "files": [
          "images/favicon.ico"
        ],
        "src": ".",
        "dest": "."
      }
    ],
    "output": "../_site",
    "template": [
      "default",
	  "modern",
	  "templates/aspose-modern"
    ],
    "globalMetadata": {
      "_appName": "Aspose.CAD for .NET",
      "_appTitle": "Aspose.CAD for .NET API Reference",
      "_enableSearch": true,
      "pdf": true
    },
	"sitemap": {
      "baseUrl": "https://reference.aspose.com/cad/net/",
      "priority": 0.1,
      "changefreq": "monthly"
    }
  }
}