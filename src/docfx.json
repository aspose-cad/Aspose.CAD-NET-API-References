{
  "metadata": [
    {
      "src": [
        {
          "src": "bootstrap",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "../api"
    }
  ],
  "build": {
    "content": [
      {
        "files": {
          "../content/**/*.{md,yml}",
		  "../api/**/*.{md,yml}"
        ],
        "exclude": [
          "../_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "../images/**"
        ]
      }
    ],
    "output": "../_site",
    "template": [
      "default",
      "modern"
    ],
	"sitemap": {
      "baseUrl": "https://reference.aspose.com/cad/net/",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "globalMetadata": {
      "_appName": "Aspose.CAD for .NET",
      "_appTitle": "Aspose.CAD for .NET API Reference",
      "_enableSearch": true,
      "pdf": true
    }
  }
}